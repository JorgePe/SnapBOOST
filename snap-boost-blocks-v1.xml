    <block-definition
            s="move1motor %&apos;motor&apos; for %&apos;milliseconds&apos; milliseconds at %&apos;speed&apos; speed @ %&apos;IPaddress&apos; : %&apos;Port&apos;"
            type="command" category="motion">
        <header></header>
        <code></code>
        <inputs>
            <input type="%txt">a</input>
            <input type="%n">100</input>
            <input type="%n">50</input>
            <input type="%s">192.168.1.100</input>
            <input type="%s">8001</input>
        </inputs>
        <script>
            <block s="doDeclareVariables">
                <list>
                    <l>http_address</l>
                </list>
            </block>
            <block s="doSetVar">
                <l>http_address</l>
                <block s="reportJoinWords">
                    <list>
                        <block var="IPaddress"/>
                        <l>:</l>
                        <block var="Port"/>
                        <l>/</l>
                    </list>
                </block>
            </block>
            <block s="doIf">
                <block s="reportAnd">
                    <block s="reportAnd">
                        <block s="reportAnd">
                            <block s="reportLessThan">
                                <l>-101</l>
                                <block var="speed"/>
                            </block>
                            <block s="reportLessThan">
                                <block var="speed"/>
                                <l>101</l>
                            </block>
                        </block>
                        <block s="reportLessThan">
                            <block var="milliseconds"/>
                            <l>999</l>
                        </block>
                    </block>
                    <block s="reportListContainsItem">
                        <block s="reportNewList">
                            <list>
                                <l>a</l>
                                <l>b</l>
                                <l>c</l>
                                <l>d</l>
                            </list>
                        </block>
                        <block var="motor"/>
                    </block>
                </block>
                <script>
                    <block s="doIfElse">
                        <block s="reportNot">
                            <block s="reportEquals">
                                <block s="reportMonadic">
                                    <l>
                                        <option>abs</option>
                                    </l>
                                    <block var="speed"/>
                                </block>
                                <block var="speed"/>
                            </block>
                        </block>
                        <script>
                            <block s="doRun">
                                <block s="reifyReporter">
                                    <autolambda>
                                        <block s="reportURL">
                                            <block s="reportJoinWords">
                                                <list>
                                                    <block var="http_address"/>
                                                    <l>move1motor</l>
                                                    <block var="motor"/>
                                                    <l>x</l>
                                                    <block var="milliseconds"/>
                                                    <l>x</l>
                                                    <block s="reportMonadic">
                                                        <l>
                                                            <option>abs</option>
                                                        </l>
                                                        <block var="speed"/>
                                                    </block>
                                                    <l>x</l>
                                                    <l>-</l>
                                                </list>
                                            </block>
                                        </block>
                                    </autolambda>
                                    <list></list>
                                </block>
                                <list></list>
                            </block>
                        </script>
                        <script>
                            <block s="doRun">
                                <block s="reifyReporter">
                                    <autolambda>
                                        <block s="reportURL">
                                            <block s="reportJoinWords">
                                                <list>
                                                    <block var="http_address"/>
                                                    <l>move1motor</l>
                                                    <block var="motor"/>
                                                    <l>x</l>
                                                    <block var="milliseconds"/>
                                                    <l>x</l>
                                                    <block s="reportMonadic">
                                                        <l>
                                                            <option>abs</option>
                                                        </l>
                                                        <block var="speed"/>
                                                    </block>
                                                    <l>x</l>
                                                    <l>+</l>
                                                </list>
                                            </block>
                                        </block>
                                    </autolambda>
                                    <list></list>
                                </block>
                                <list></list>
                            </block>
                        </script>
                    </block>
                </script>
            </block>
        </script>
    </block-definition>
